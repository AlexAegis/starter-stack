{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"types": ["node", "jest"],
		"paths": {
			"@app": ["apps/demo-app/src/app"],
			"@app/*": ["apps/demo-app/src/app/*"],
			"@api": ["apps/demo-app/src/api"],

{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "",
	"projects": {
		"starter-stack": {
			"projectType": "application",
			"schematics": {
				"@nrwl/workspace:component": {
					"style": "scss"
				}
			},
			"root": "apps/starter-stack",
			"sourceRoot": "apps/starter-stack/src",
			"prefix": "starter-stack",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/apps/starter-stack",
						"index": "apps/starter-stack/src/index.html",
						"main": "apps/starter-stack/src/main.ts",
						"polyfills": "apps/starter-stack/src/polyfills.ts",
						"tsConfig": "apps/starter-stack/tsconfig.app.json",
						"assets": ["apps/starter-stack/src/favicon.ico", "apps/starter-stack/src/assets"],
						"styles": ["apps/starter-stack/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/starter-stack/src/environments/environment.ts",
									"with": "apps/starter-stack/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "starter-stack:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "starter-stack:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "starter-stack:build"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": ["apps/starter-stack/tsconfig.app.json", "apps/starter-stack/tsconfig.spec.json"],
						"exclude": ["**/node_modules/**", "!apps/starter-stack/**"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "apps/starter-stack/jest.config.js",
						"tsConfig": "apps/starter-stack/tsconfig.spec.json",
						"setupFile": "apps/starter-stack/src/test-setup.ts"
					}
				}
			}
		},
		"starter-stack-e2e": {
			"root": "apps/starter-stack-e2e",
			"sourceRoot": "apps/starter-stack-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/starter-stack-e2e/cypress.json",
						"tsConfig": "apps/starter-stack-e2e/tsconfig.e2e.json",
						"devServerTarget": "starter-stack:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "starter-stack:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": "apps/starter-stack-e2e/tsconfig.e2e.json",
						"exclude": ["**/node_modules/**", "!apps/starter-stack-e2e/**"]
					}
				}
			}
		},
		"demo-server": {
			"root": "apps/demo-server",
			"sourceRoot": "apps/demo-server/src",
			"projectType": "application",
			"prefix": "demo-server",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@nrwl/node:build",
					"options": {
						"outputPath": "dist/apps/demo-server",
						"main": "apps/demo-server/src/main.ts",
						"tsConfig": "apps/demo-server/tsconfig.app.json",
						"assets": ["apps/demo-server/src/assets"]
					},
					"configurations": {
						"production": {
							"optimization": true,
							"extractLicenses": true,
							"inspect": false,
							"fileReplacements": [
								{
									"replace": "apps/demo-server/src/environments/environment.ts",
									"with": "apps/demo-server/src/environments/environment.prod.ts"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@nrwl/node:execute",
					"options": {
						"buildTarget": "demo-server:build"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": ["apps/demo-server/tsconfig.app.json", "apps/demo-server/tsconfig.spec.json"],
						"exclude": ["**/node_modules/**", "!apps/demo-server/**"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "apps/demo-server/jest.config.js",
						"tsConfig": "apps/demo-server/tsconfig.spec.json"
					}
				}
			}
		}
	},
	"cli": {
		"warnings": {
			"typescriptMismatch": false,
			"versionMismatch": false
		},
		"defaultCollection": "@nrwl/angular"
	},
	"schematics": {
		"@nrwl/angular:application": {
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"unitTestRunner": "jest"
		}
	},
	"defaultProject": "starter-stack"
}

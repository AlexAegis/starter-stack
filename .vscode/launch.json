{
	"configurations": [
		{
			"type": "chrome",
			"linux": {
				"runtimeExecutable": "/usr/bin/chromium"
			},
			"request": "launch",
			"name": "Launch, No Serve",
			"url": "http://localhost:4201",
			"webRoot": "${workspaceFolder}",
			"cwd": "${workspaceFolder}",
			"userDataDir": "${workspaceFolder}/.vscode/chrome/"
		},
		{
			"type": "chrome",
			"linux": {
				"runtimeExecutable": "/usr/bin/chromium"
			},
			"request": "launch",
			"name": "Launch All",
			"url": "http://localhost:4201",
			"webRoot": "${workspaceFolder}",
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "start:all",
			"userDataDir": "${workspaceFolder}/.vscode/chrome/"
		},
		{
			"type": "chrome",
			"linux": {
				"runtimeExecutable": "/usr/bin/chromium"
			},
			"request": "launch",
			"name": "Launch App",
			"url": "http://localhost:4201",
			"webRoot": "${workspaceFolder}",
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "start:app",
			"userDataDir": "${workspaceFolder}/.vscode/chrome/"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Api",
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "start:api"
		}
	]
}
